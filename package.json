{
  "name": "aria-patterns",
  "version": "1.0.0",
  "description": "Common patterns of elements with ARIA attributes.",
  "main": "index.js",
  "scripts": {
    "build": "gulp build",
    "unbuild:address": "rm -r ./x-address/{css/dist/,test/out/}",
    "unbuild:directory": "rm -r ./x-directory/{css/dist/,test/out/}",
    "unbuild:hidden": "rm -r ./x-hidden/css/dist/",
    "unbuild:permalink": "rm -r ./x-permalink/{css/dist/,test/out/}",
    "unbuild:person-fullname": "rm -r ./x-person-fullname/{css/dist/,test/out/}",
    "unbuild:tablist": "rm -r ./x-tablist/{css/dist/,js/dist/}",
    "unbuild:each": "npm run unbuild:address  &&  npm run unbuild:directory  &&  npm run unbuild:hidden  &&  npm run unbuild:permalink  &&  npm run unbuild:person-fullname  &&  npm run unbuild:tablist",
    "unbuild": "rm -r ./docs/{styleguide,api}/  &&  npm run unbuild:each",
    "test": "gulp test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chharvey/aria-patterns.git"
  },
  "keywords": [
    "aria",
    "a11y",
    "accessibility",
    "components"
  ],
  "author": "Chris Harvey <chrisharvey2pi@gmail.com> (https://chharvey.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chharvey/aria-patterns/issues"
  },
  "homepage": "https://github.com/chharvey/aria-patterns#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.5.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-clean-css": "^3.9.4",
    "gulp-inject-string": "^1.1.1",
    "gulp-jsdoc3": "^2.0.0",
    "gulp-less": "^4.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typedoc": "^2.2.0",
    "gulp-typescript": "^5.0.0-alpha.3",
    "kss": "^3.0.0-beta.22",
    "make-dir": "^1.3.0",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "@types/make-dir": "^1.0.3",
    "@types/node": "^10.12.10",
    "extrajs": "^0.15.0",
    "extrajs-dom": "^5.0.0-alpha.3",
    "extrajs-geo": "^0.1.0",
    "schemaorg-jsd": "^0.13.0",
    "template-processor": "^1.0.1"
  }
}
